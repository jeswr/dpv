  <rdf:Description rdf:about="https://w3id.org/dpv/owl#IndeterminateDuration">
    <skos:prefLabel xml:lang="en">Indeterminate Duration</skos:prefLabel>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-11-30</dct:created>

    sw:term_status "accepted"@en ;
    skos:definition "Authority associated with the main or local establishment of an organisation"@en ;
    skos:prefLabel "Local Supervisory Authority"@en .

eu-gdpr-owl:MainEstablishment a rdfs:Class,
        owl:Class ;
    dct:contributor "Harshvardhan J. Pandit, Georg P. Krog" ;
    dct:created "2024-02-14"^^xsd:date ;
    dct:source " GDPR Art.56"@en,
        " Guidelines 3/2018 on the territorial scope of the GDPR (Article 3) Version 2.1"@en,
        "GDPR Art.4-16"@en ;
    rdfs:isDefinedBy eu-gdpr-owl: ;
    rdfs:subClassOf eu-gdpr-owl:Establishment ;
    sw:term_status "accepted"@en ;
    skos:definition "A Main Establishment is the place of central administration in the Union unless the decisions on the purposes and means of the processing of personal data are taken in another establishment in the Union and the latter establishment has the power to have such decisions implemented, in which case the establishment having taken such decisions is to be considered to be the main establishment"@en ;
    skos:prefLabel "Main Establishment"@en .

eu-gdpr-owl:ProcessorBreachNotice a rdfs:Class,
        owl:Class,
        eu-gdpr-owl:DataBreachNotice ;
    dct:created "2024-05-19"^^xsd:date ;
    dct:source "GDPR Article 33"@en ;
    rdfs:isDefinedBy eu-gdpr-owl: ;
    rdfs:subClassOf eu-gdpr-owl:DataBreachNotice ;
    sw:term_status "accepted"@en ;
    skos:definition "Notice regarding a data breach to the Processor"@en ;
    skos:prefLabel "Processor Breach Notice"@en .

eu-gdpr-owl:ProcessorBreachNotificationNeeded a rdfs:Class,
        owl:Class,
        eu-gdpr-owl:DataBreachNoticeRequirement ;
    dct:created "2024-05-19"^^xsd:date ;
    dct:source "GDPR Article 33"@en ;
    rdfs:isDefinedBy eu-gdpr-owl: ;
    rdfs:subClassOf eu-gdpr-owl:DataBreachNoticeRequirement ;
    sw:term_status "accepted"@en ;
    skos:definition "Data Breach notification to the Processor is required"@en ;
    skos:prefLabel "Processor Breach Notification Needed"@en .

eu-gdpr-owl:PurposeLimitationPrinciple a rdfs:Class,
        owl:Class,
        dpv-owl:OrganisationalMeasure ;
    dct:contributor "Georg P. Krog" ;
    dct:created "2024-05-12"^^xsd:date ;
    dct:source "GDPR Art.5-1b"@en ;
    rdfs:isDefinedBy eu-gdpr-owl: ;
    rdfs:subClassOf dpv-owl:Principle ;
    sw:term_status "accepted"@en ;
    skos:definition "Principle stating personal data collected for specified, explicit and legitimate purposes and not further processed in a manner that is incompatible with those purposes; further processing for archiving purposes in the public interest, scientific or historical research purposes or statistical purposes shall, in accordance with Article 89(1), not be considered to be incompatible with the initial purposes"@en ;
    skos:prefLabel "Purpose Limitation Principle"@en .

eu-gdpr-owl:RightsRecipientsNotice a rdfs:Class,
        owl:Class,
        dpv-owl:OrganisationalMeasure ;
    dct:contributor "Beatriz Esteves, Georg P. Krog, Harshvardhan J. Pandit" ;
    dct:created "2022-11-09"^^xsd:date ;
    rdfs:isDefinedBy eu-gdpr-owl: ;
    rdfs:subClassOf dpv-owl:RightFulfilmentNotice ;
    sw:term_status "accepted"@en ;
    skos:definition "A Notice provided in fulfilment of GDPR's Art.19 regarding Recipients to whom a rights exercise has been communicated, such as regarding rectification (A.16) or erasure of personal data (A.17) or restriction of processing (A.18)"@en ;
    skos:prefLabel "Rights Recipients Notice"@en .

eu-gdpr-owl:SARNotice a rdfs:Class,
        owl:Class,
        dpv-owl:OrganisationalMeasure ;
    dct:contributor "Beatriz Esteves, Georg P. Krog, Harshvardhan J. Pandit" ;
    dct:created "2022-11-09"^^xsd:date ;
    rdfs:isDefinedBy eu-gdpr-owl: ;
    rdfs:subClassOf dpv-owl:RightFulfilmentNotice ;
    sw:term_status "accepted"@en ;
    skos:definition "A Notice provided in fulfilment of GDPR's Art.15 regarding information to be provided for Right of Access or Subject Access Request (SAR)"@en ;
    skos:prefLabel "SAR Notice"@en .

eu-gdpr-owl:SCCByCommission a rdfs:Class,
        owl:Class,
        dpv-owl:OrganisationalMeasure ;
    dct:contributor "David Hickey, Paul Ryan, Georg P. Krog, Harshvardhan J. Pandit" ;
    dct:created "2021-09-22"^^xsd:date ;
    dct:source [ a schema:WebPage ;
            schema:name "GDPR Art.46-2c" ;
            schema:url "https://eur-lex.europa.eu/eli/reg/2016/679/art_46/par_2/pnt_c/oj" ] ;
    rdfs:isDefinedBy eu-gdpr-owl: ;
    rdfs:subClassOf eu-gdpr-owl:DataTransferTool,
        eu-gdpr-owl:StandardContractualClauses ;
    sw:term_status "accepted"@en ;
    skos:definition "Standard contractual clauses adopted by the Commission in accordance with the examination procedure referred to in GDPR Article 93(2)"@en ;
    skos:prefLabel "SCCs adopted by Commission"@en .

eu-gdpr-owl:SCCBySupervisoryAuthority a rdfs:Class,
        owl:Class,
        dpv-owl:OrganisationalMeasure ;
    dct:contributor "David Hickey, Paul Ryan, Georg P. Krog, Harshvardhan J. Pandit" ;
    dct:created "2021-09-22"^^xsd:date ;
    dct:source [ a schema:WebPage ;
            schema:name "GDPR Art.46-2d" ;

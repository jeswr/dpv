eu-aiact-owl:RealWorldTestingPlan a rdfs:Class,
        owl:Class,
        tech-owl:Documentation ;
    dct:contributor "Delaram Golpayegani" ;
    dct:created "2024-04-10"^^xsd:date ;
    dct:source "AIA (Art. 3(bf))"@en ;

      }
    ],
    "http://www.w3.org/2004/02/skos/core#definition": [
      {
        "@language": "en",
        "@value": "Consent that is expressed through an explicit action solely conveying a consenting decision"
      }
    ],
    "http://www.w3.org/2004/02/skos/core#inScheme": [

    Types: 
        <https://w3id.org/dpv/2.0/owl#EvaluationScoring>
    
    
Individual: <https://w3id.org/dpv/2.0/owl#SystematicMonitoring>

    Annotations: 
        rdfs:isDefinedBy <https://w3id.org/dpv/2.0/owl#>,
        skos:definition "Processing that involves systematic monitoring of individuals"@en,
        skos:prefLabel "Systematic Monitoring"@en
    
    Types: 
        <https://w3id.org/dpv/2.0/owl#ProcessingContext>
    
    

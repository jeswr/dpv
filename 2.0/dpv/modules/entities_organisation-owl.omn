        skos:prefLabel "Academic or Scientific Organisation"@en
    
    SubClassOf: 
        <https://w3id.org/dpv/2.0/owl#Organisation>
    
    
Class: <https://w3id.org/dpv/2.0/owl#ForProfitOrganisation>

    Annotations: 
        rdfs:isDefinedBy <https://w3id.org/dpv/2.0/owl#>,
        skos:definition "An organisation that aims to achieve profit as its primary goal"@en,
        skos:prefLabel "For-Profit Organisation"@en
    
    SubClassOf: 
        <https://w3id.org/dpv/2.0/owl#Organisation>
    
    
Class: <https://w3id.org/dpv/2.0/owl#GovernmentalOrganisation>

    Annotations: 
        rdfs:isDefinedBy <https://w3id.org/dpv/2.0/owl#>,
        skos:definition "An organisation managed or part of government"@en,
        skos:prefLabel "Governmental Organisation"@en
    
    SubClassOf: 
        <https://w3id.org/dpv/2.0/owl#Organisation>
    
    
Class: <https://w3id.org/dpv/2.0/owl#IndustryConsortium>


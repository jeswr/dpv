    <skos:definition xml:lang="en">Data that has been provided by an entity</skos:definition>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-04-20</dct:created>
    <dct:contributor>Harshvardhan J. Pandit, Paul Ryan</dct:contributor>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <skos:prefLabel xml:lang="en">Provided Data</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#DerivedPersonalData">
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2019-05-07</dct:created>
    <dct:contributor>Elmar Kiesling; Harshvardhan J. Pandit, Fajar Ekaputra</dct:contributor>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:prefLabel xml:lang="en">Derived Personal Data</skos:prefLabel>
    <vann:example rdf:resource="https://w3id.org/dpv/examples/owl#E0009"/>
    <vann:example rdf:resource="https://w3id.org/dpv/examples/owl#E0046"/>
    <skos:definition xml:lang="en">Personal Data that is obtained or derived from other data</skos:definition>
    <skos:related rdf:resource="https://specialprivacy.ercim.eu/vocabs/data#Derived"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:source rdf:nodeID="N7133e1a857324544bb17873388b1923e"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#DerivedData"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#PersonalData"/>
    <skos:scopeNote xml:lang="en">Derived Data is data that is obtained through processing of existing data, e.g. deriving first name from full name. To indicate data that is derived but which was not present or evident within the source data, InferredPersonalData should be used.</skos:scopeNote>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-12-10</dct:modified>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/owl#PseudonymisedData">
    <skos:prefLabel xml:lang="en">Pseudonymised Data</skos:prefLabel>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/owl#PersonalData"/>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-01-19</dct:created>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/owl#"/>
    <skos:definition xml:lang="en">Pseudonymised Data is data that has gone a partial or incomplete anonymisation process by replacing the identifiable information with artificial identifiers or 'pseudonyms', and is still considered as personal data</skos:definition>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
  </rdf:Description>

    skos:prefLabel "Frequency"@en .

dpv-owl:Importance a rdfs:Class,
        owl:Class ;
    dct:contributor "Harshvardhan J. Pandit, Paul Ryan, Georg P. Krog, Julian Flake, Beatriz Esteves" ;

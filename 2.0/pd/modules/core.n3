    dct:source [ a schema:WebPage ;
            schema:name "EnterPrivacy Categories of Personal Information" ;
            schema:url "https://enterprivacy.com/wp-content/uploads/2018/09/Categories-of-Personal-Information.pdf" ] ;
    rdfs:isDefinedBy pd: ;
    sw:term_status "accepted"@en ;
    skos:broader dpv:PersonalData ;

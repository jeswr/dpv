    rdfs:subClassOf eu-dga-owl:DataIntermediationService ;
    sw:term_status "accepted"@en ;
    skos:definition "Service provided by a data cooperative"@en ;
    skos:prefLabel "Data Cooperative Service"@en .

eu-dga-owl:DataIntermediationService a rdfs:Class,
        owl:Class ;
    dct:contributor "Beatriz Esteves" ;
    dct:created "2024-02-14"^^xsd:date ;

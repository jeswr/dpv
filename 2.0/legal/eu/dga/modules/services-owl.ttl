eu-dga-owl:DataIntermediationServiceBetweenHoldersUsers a rdfs:Class,
        owl:Class ;
    dct:contributor "Beatriz Esteves" ;
    dct:created "2024-02-14"^^xsd:date ;
    dct:source "DGA 10.a"@en ;

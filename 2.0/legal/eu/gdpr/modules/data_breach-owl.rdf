  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr/owl#DataBreach">
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/gdpr/owl#"/>
    <skos:scopeNote xml:lang="en">GDPR's notion of data breach includes any incident that affects the confidentiality, integrity, and availability of personal data and its processing without distinguishing between internal or external actors involved in the incident</skos:scopeNote>
    <skos:prefLabel xml:lang="en">Data Breach</skos:prefLabel>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/risk/owl#Incident"/>
    <dct:source xml:lang="en">GDPR Article 4(12)</dct:source>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-05-19</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:definition xml:lang="en">A breach of security leading to the accidental or unlawful destruction, loss, alteration, unauthorised disclosure of, or access to, personal data transmitted, stored or otherwise processed</skos:definition>
  </rdf:Description>

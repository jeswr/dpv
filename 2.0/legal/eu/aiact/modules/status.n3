eu-aiact:RecalledFromService a rdfs:Class,
        skos:Concept,
        eu-aiact:ServiceSupplyStatus ;
    dct:contributor "Delaram Golpayegani" ;
    dct:created "2024-05-17"^^xsd:date ;
    rdfs:isDefinedBy eu-aiact: ;
    sw:term_status "accepted"@en ;
